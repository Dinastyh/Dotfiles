filetype on

syntax enable

set smarttab

set autoindent

set number

set backspace=indent,eol,start

set clipboard=unnamedplus

set nocompatible

set smartcase

set autochdir

set wildmode=longest,list,full

set splitbelow splitright

let mapleader = " "

execute pathogen#infect()

filetype plugin indent on

colorscheme zellner

set shiftwidth=4
